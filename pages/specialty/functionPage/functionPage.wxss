.f0_container{
  width: 100%;
  font-size: 14px;

}
/* 头部装饰 */
.f0_background{
  width: 100%;
  height: 380rpx;
  border: 0px solid black;
}
.f0_logo_background_red{
  width: 600rpx;
  height: 600rpx;
  background-color: #A25D6D;
  border-radius: 50%;
  position: absolute;
}
.f0_logo_img{
  width: 180rpx;
  height: 180rpx;
  margin: 0 auto;
  left: 0;
  right: 0;
  border: 2rpx solid black;
  position: absolute;
  z-index: 999;
}
.f0_logo_text1{
  width: 100%;
  color: white;
  font-size: 40rpx;
  letter-spacing: 3rpx;
  text-align: center;
  position: absolute;
  top: 200rpx;
  z-index: 998;
}
.f0_logo_text2{
  width: 100%;
  color: white;
  font-size: 22rpx;
  letter-spacing: 4rpx;
  text-align: center;
  position: absolute;
  top: 260rpx;
  z-index: 998;
}

/* 选项卡样式（容器） */
.f0_choose{
  width: 560rpx;
  height: 120rpx;
  border-radius: 60rpx;
  border: 2px solid #A25D6D;
  margin: 60rpx auto;
  overflow: hidden;
}
/* 选项卡左样式（容器） */
.f0_choose_left{
  width: 120rpx;
  height: 120rpx;
  color: #9D2036;
  text-align: center;
  line-height: 120rpx;
  margin-left: 20rpx;
  float: left;
}
/* 选项卡左样式（容器中的图标） */
.f0_choose_left_icon1{
  background: url("data:image/svg+xml;base64,") no-repeat center center;
  background-size: 66rpx 66rpx;
}
.f0_choose_left_icon2{
  background: url("data:image/svg+xml;base64,") no-repeat center center;
  background-size: 66rpx 66rpx;
}
.f0_choose_left_icon3{
  background: url("data:image/svg+xml;base64,") no-repeat center center;
  background-size: 66rpx 66rpx;
}
/* 选项卡右样式（容器） */
.f0_choose_right{
  width: 400rpx;
  height: 120rpx;
  float: left;
  display: flex;
  flex-direction: column;
  align-items: left;
  justify-content:center;
}
/* 选项卡右样式（容器中的文字 课程信息/专业方向/就业形势...） */
.f0_choose_right_text1{
  font-size: 36rpx;
  /* width: 170rpx; */
  /* height: 120rpx; */
  color: #A12D41;
  border-left: 2px solid #8E9782;
  padding-left: 30rpx;
  display: flex;
  flex-direction: column;
  align-items: left;
  justify-content:center;
}
/* 选项卡右样式（容器中的文字 数据来源...） */
.f0_choose_right_text2{
  font-size: 21rpx;
  color: #C8A7B0;
  border-left: 2px solid #8E9782;
  padding-left: 30rpx;
  display: flex;
  flex-direction: column;
  align-items: left;
  justify-content:center;
}
/* 底部权利声明 */
.f0_foot{
  width: 100%;
  font-size: 24rpx;
  color: #8E9782;
  text-align: center;
  position: absolute;
  bottom: 50rpx;
}